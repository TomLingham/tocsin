{
  "collectCoverageFrom": ["src/**/*.{ts,tsx,js,jsx}", "!**/__test__/**"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "testMatch": ["<rootDir>/**/__test__/**/*.test.ts?(x)"],
  "transform": { "^.+\\.(ts|tsx)$": "babel-jest" },
  "setupFilesAfterEnv": ["<rootDir>/test/jest.setup.ts"]
}
