{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended"
  ],
  "env": {
    "amd": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "curly": "error",
    "no-unused-vars": "off",
    "require-atomic-updates": "off"
  }
}
